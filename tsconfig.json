{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "ES2015",
		"moduleResolution": "bundler"
	},
	"exclude": ["eslint.config.js", "postcss.config.js", "svelte.config.js", "node_modules"]
}
